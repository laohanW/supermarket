<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-type" content="text/html;charset=utf-8">
        <title><%= htmlWebpackPlugin.options.title%></title>
        
    </head>
    <body>
        <div id='content'></div>
        <br/>
        <div class="css-webpack">
            <% for(var css in htmlWebpackPlugin.files.css){ %>
                <p><% htmlWebpackPlugin.files.css[css] %> </p>
            <% } %>
        </div>
        <div class="js-webpack">
            <% for(var chunk in htmlWebpackPlugin.files.chunks){ %>
                <br/>
                <p>entry: <% htmlWebpackPlugin.files.chunks[chunk].entry %></p>
                <p>css:<% htmlWebpackPlugin.files.chunks[chunk].css %></p>
            <% } %>
        </div>
    </body>
</html>
